{
  "name": "srv-cli",
  "version": "0.3.6",
  "description": "A modern, opinionated and simple microservices stack built on Express.",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "test",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "require": [
      "babel-core/register"
    ]
  },
  "bin": {
    "srv": "build/srv"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "gulp": "gulp",
    "prepublish": "npm run gulp build",
    "test": "gulp build && nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfg/srv.git"
  },
  "keywords": [
    "node",
    "microservices",
    "express"
  ],
  "author": "Alfred Gutierrez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alfg/srv/issues"
  },
  "homepage": "https://github.com/alfg/srv",
  "dependencies": {
    "apidoc": "^0.17.6",
    "babel-plugin-module-alias": "^1.5.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "6.26.0",
    "chalk": "^2.2.0",
    "commander": "^2.9.0",
    "cors": "^2.7.1",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "update-notifier": "^2.3.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-register": "6.26.0",
    "coveralls": "^3.0.0",
    "del": "^3.0.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-cached": "^1.1.0",
    "gulp-eslint": "^4.0.2",
    "gulp-ext": "^1.0.0",
    "gulp-task-listing": "^1.0.1",
    "nyc": "12.0.2",
    "request-promise": "4.2.2"
  }
}
